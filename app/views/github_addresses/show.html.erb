

<p >
  <h3 class="text-info">Github Address:</h3>
  <a href = <%= @github_address.githubadd %>> <%= @github_address.githubadd %> </a>
</p>
<h4 class="text-info">Projects</h4>
<% @github_address.projects.each do |c| %>
  <p class="text-error">
    <b>Project:</b>
    <a href = <%= c.projecturl %> ><%= c.projecturl %> </a>
  </p>
 
  <% end %>

<h4 class="text-info">Add a Projects:</h4>
<%= simple_form_for([@github_address, @github_address.projects.build]) do |f| %>
  <div class="field text-info">
    <%= f.label :projecturl %><br />
    <%= f.text_field :projecturl %>
  </div>
  
  
    <%= f.submit :class => "btn-primary",:label => "Add Project" %>
  
<% end %>



<%= link_to 'Edit', edit_github_address_path(@github_address) %> |
<%= link_to 'Back', github_addresses_path %>
